<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.gwc" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Competence Assessment Tool </name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@infosol.com" href="http://cordova.io">
      Infosol Development Team
    </author>
    <content src="index.html" />
    <access origin="http://google.com" />
    <access origin="https://google.com" />
    <access origin="192.168.1.7:3000/competences" />
    <access origin="http://maps.google.com" />
    <access origin="http://*.google.com" />
    <access origin="content:///*" />
    <access origin="*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="http://example.com/*" />
    <allow-intent href="*://*.example.com/*" />
    <allow-intent href="sms:*" />
    <allow-intent href="tel:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
</widget>
